   "source": [
    "l = list()\n",
    "numbers = input()\n",
    "\n",
    "while numbers!='':\n",
    "    l.append(int(numbers))\n",
    "    numbers = input()\n",
    "\n",
    "l.sort()\n",
    "print(l)\n",
    "\n",
    "if len(l) % 2 == 0:\n",
    "    print(float(l[len(l)//2]))\n",
    "\n",
    "elif len(l) % 2 == 1:\n",
    "    print(float(((l[len(l)//2-1]) + (l[len(l)//2+1]))/2))\n",
    "\n",
    "\n"
   ]
